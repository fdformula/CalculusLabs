            RowBox[{"[", "1", "]"}], "]"}], ",", 
           RowBox[{"f", "[", 
            RowBox[{"xs", "[", 
             RowBox[{"[", "1", "]"}], "]"}], "]"}]}], "}"}], "]"}]}], "}"}], 
      "]"}], ",", "\[IndentingNewLine]", 
     RowBox[{"Graphics", "[", 
      RowBox[{"{", 
