de473bdfc3e5"],

Cell[CellGroupData[{

Cell[BoxData[{
 RowBox[{
  RowBox[{"f", "[", "x_", "]"}], ":=", 
  RowBox[{"x", "+", 
   RowBox[{"Cos", "[", "x", "]"}]}]}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"g", "[", "x_", "]"}], "=", 
   RowBox[{
    RowBox[{"f", "''"}], "[", "x", "]"}]}], ";"}], "\[IndentingNewLine]", 
