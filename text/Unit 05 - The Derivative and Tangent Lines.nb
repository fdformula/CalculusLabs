Cell["\<\
The following function allows you to explore the tangent lines of a function \
at different points.\
\>", "Text",
 CellChangeTimes->{{3.6429432412799625`*^9, 
  3.642943279457694*^9}},ExpressionUUID->"ae19e318-d29f-447a-b8ec-\
20e328128120"],

Cell[BoxData[
 RowBox[{
  RowBox[{"ExploreTangentLine", "[", 
   RowBox[{"func_", ",", 
    RowBox[{"{", 
     RowBox[{"x_", ",", "xmin_", ",", "xmax_"}], "}"}], ",", "plotRange_"}], 
   "]"}], " ", ":=", "\[IndentingNewLine]", 
  RowBox[{"Manipulate", "[", "\[IndentingNewLine]", " ", 
   RowBox[{
    RowBox[{"Module", "[", "\[IndentingNewLine]", 
     RowBox[{
      RowBox[{"{", 
       RowBox[{"f", ",", " ", "fPrime"}], "}"}], ",", "\[IndentingNewLine]", 
      RowBox[{
       RowBox[{
        RowBox[{"f", "[", "X_", "]"}], "=", 
        RowBox[{"func", "/.", 
         RowBox[{"x", "\[Rule]", "X"}]}]}], ";", "\[IndentingNewLine]", 
       RowBox[{
        RowBox[{"fPrime", "[", "X_", "]"}], "=", 
        RowBox[{
         RowBox[{"f", "'"}], "[", "X", "]"}]}], ";", "\[IndentingNewLine]", 
       "\[IndentingNewLine]", 
       RowBox[{"Show", "[", 
        RowBox[{"{", "\[IndentingNewLine]", 
         RowBox[{
          RowBox[{"Plot", "[", 
           RowBox[{
            RowBox[{"{", 
             RowBox[{
              RowBox[{"f", "[", "x", "]"}], ",", 
              RowBox[{"fPrime", "[", "x", "]"}], ",", 
              RowBox[{
               RowBox[{"f", "[", "x0", "]"}], "+", 
               RowBox[{
                RowBox[{"fPrime", "[", "x0", "]"}], 
                RowBox[{"(", 
                 RowBox[{"x", "-", "x0"}], ")"}]}]}]}], "}"}], ",", 
            RowBox[{"{", 
             RowBox[{"x", ",", "xmin", ",", "xmax"}], "}"}], ",", 
            RowBox[{"PlotStyle", "\[Rule]", 
             RowBox[{"{", 
              RowBox[{
               RowBox[{"{", "}"}], ",", 
               RowBox[{"{", 
                RowBox[{"Dashed", ",", " ", "Gray"}], "}"}], ",", 
               RowBox[{"{", "}"}]}], "}"}]}], ",", "plotRange", ",", 
            RowBox[{"ImageSize", "\[Rule]", "320"}]}], "]"}], ",", 
          "\[IndentingNewLine]", 
          RowBox[{"Graphics", "[", 
           RowBox[{"{", 
            RowBox[{"Red", ",", 
             RowBox[{"PointSize", "[", "Medium", "]"}], ",", 
             RowBox[{"Point", "[", 
              RowBox[{"{", 
               RowBox[{"x0", ",", 
                RowBox[{"f", "[", "x0", "]"}]}], "}"}], "]"}]}], "}"}], 
           "]"}]}], "\[IndentingNewLine]", "}"}], "]"}]}]}], "]"}], ",", 
    "\[IndentingNewLine]", 
    RowBox[{"{", 
     RowBox[{
      RowBox[{"{", 
       RowBox[{"x0", ",", 
        RowBox[{"xmin", "+", 
         RowBox[{
          RowBox[{"(", 
           RowBox[{"xmax", "-", "xmin"}], ")"}], "/", "3"}]}], ",", 
        RowBox[{"Style", "[", 
         SubscriptBox["x", "0"], "]"}]}], "}"}], ",", "xmin", ",", "xmax"}], 
     "}"}]}], "\[IndentingNewLine]", "]"}]}]], "Input",
 CellChangeTimes->{{3.6429432855837545`*^9, 3.642943441645472*^9}, {
