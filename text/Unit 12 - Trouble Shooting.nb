Cell[TextData[{
 "Obviously, this is not the answer, ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"4", "x"}], "+", "3"}], TraditionalForm]],ExpressionUUID->
  "8b359889-876b-4ac7-aeb1-976f79e69a67"],
