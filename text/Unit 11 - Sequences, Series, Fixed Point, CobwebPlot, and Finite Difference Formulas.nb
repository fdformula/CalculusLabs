  FormBox[
   RowBox[{
    RowBox[{"f", "(", "x", ")"}], "=", 
    RowBox[{"1", "-", 
     RowBox[{"4", 
      SuperscriptBox[
       RowBox[{"(", 
        RowBox[{"x", "-", 
         FractionBox["1", "2"]}], ")"}], "2"]}]}]}], TraditionalForm]],
  ExpressionUUID->"667e3ca9-5f1e-49c2-bb62-9000eb23b70c"],
 ", and ",
