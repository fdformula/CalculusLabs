Cell[TextData[Cell[BoxData[
 FormBox[
  RowBox[{
   RowBox[{
    SubscriptBox["x", 
     RowBox[{"i", "+", "1"}]], "=", 
    RowBox[{
     SubscriptBox["x", "i"], "-", 
     FractionBox[
      RowBox[{"f", "(", 
       SubscriptBox["x", "i"], ")"}], 
      RowBox[{
       RowBox[{"f", "'"}], 
       RowBox[{"(", 
        SubscriptBox["x", "i"], ")"}]}]]}]}], ",", 
   RowBox[{"i", "=", "1"}], ",", "2", ",", "\[CenterEllipsis]"}], 
  TraditionalForm]],
 FontColor->GrayLevel[
  0],ExpressionUUID->"9dc43e9a-fdeb-4a2e-ba6a-1e9d6dcfadd6"]], "Text",
