 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox["x", "i"], "\[LessEqual]", "x", "\[LessEqual]", 
    SubscriptBox["x", 
     RowBox[{"i", "+", "2"}]]}], TraditionalForm]],ExpressionUUID->
  "65d09710-cd9e-4b29-bbbf-aa6651abce50"],
 " is given by"
