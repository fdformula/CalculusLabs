Cell[TextData[{
 StyleBox["Example 10\n",
  FontWeight->"Bold"],
 "Find the area enclosed by ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"x", "=", "\[Pi]"}], ",", 
    RowBox[{"x", "=", 
     RowBox[{"2", "\[Pi]"}]}], ",", 
    RowBox[{"y", "=", "0"}]}], TraditionalForm]],ExpressionUUID->
  "17956221-d46d-4814-8c93-26594d3a9597"],
 ", and ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"f", "(", "x", ")"}], "=", 
    RowBox[{"x", " ", "sin", " ", "x"}]}], TraditionalForm]],ExpressionUUID->
  "9e843c02-a588-422d-b0c4-8b85cd0a5fc7"],
 "."
}], "Text",
 CellDingbat->"\[FilledSquare]",
 CellChangeTimes->{{3.643733543418103*^9, 3.64373354871593*^9}, {
   3.6437336066780477`*^9, 3.6437336959731617`*^9}, {3.643734164476557*^9, 
   3.643734198169326*^9}, {3.6437342530529675`*^9, 3.6437342952160754`*^9}, {
   3.64373727711145*^9, 3.643737277580277*^9}, 3.643746809693287*^9, {
   3.643921529106394*^9, 3.64392152948138*^9}, {3.645086815522339*^9, 
   3.645086815897338*^9}},ExpressionUUID->"ca4b6040-d699-4854-aa2c-\
b9c2481a12ec"],

Cell[TextData[{
 "From the graph in previous example, we know ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"f", "(", "x", ")"}], "\[LessEqual]", "0"}], TraditionalForm]],
  ExpressionUUID->"26f8a96a-c29d-4086-99ea-8916e3932b7d"],
 " and, thus, we cannot compute ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubsuperscriptBox["\[Integral]", "\[Pi]", 
     RowBox[{"2", "\[Pi]"}]], 
    RowBox[{"x", " ", "sin", " ", "x", 
     RowBox[{"\[DifferentialD]", "x"}]}]}], TraditionalForm]],ExpressionUUID->
  "df7fe8ac-5508-4a3a-ab23-38c8fb79d007"],
 " for the area."
}], "Text",
 CellChangeTimes->{{3.643734210108886*^9, 3.6437342372067175`*^9}, {
  3.6437343779638567`*^9, 3.6437344598216085`*^9}, {3.6439213220366974`*^9, 
  3.6439213288501053`*^9}},ExpressionUUID->"f22c6a33-fa0d-4f16-b057-\
62f155f1a101"],

Cell[CellGroupData[{

Cell[BoxData[
 RowBox[{"Integrate", "[", 
  RowBox[{
   RowBox[{"x", " ", 
    RowBox[{"Sin", "[", "x", "]"}]}], ",", 
   RowBox[{"{", 
    RowBox[{"x", ",", "\[Pi]", ",", 
     RowBox[{"2", "\[Pi]"}]}], "}"}]}], "]"}]], "Input",
