Cell[TextData[{
 StyleBox["Example 7\n",
  FontWeight->"Bold"],
 "Evaluate ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubsuperscriptBox["\[Integral]", 
     RowBox[{"-", "\[Infinity]"}], "\[Infinity]"], 
    RowBox[{
     FractionBox["1", 
      SqrtBox[
       RowBox[{"1", "+", 
        SuperscriptBox["x", "2"]}]]], 
     RowBox[{"\[DifferentialD]", "x"}]}]}], TraditionalForm]],ExpressionUUID->
  "ba9766c3-730e-494c-9d43-68e809f06cec"],
 ".\nRewrite ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     SubsuperscriptBox["\[Integral]", 
      RowBox[{"-", "\[Infinity]"}], "\[Infinity]"], 
     RowBox[{
      FractionBox["1", 
       SqrtBox[
        RowBox[{"1", "+", 
         SuperscriptBox["x", "2"]}]]], 
      RowBox[{"\[DifferentialD]", "x"}]}]}], "=", 
    RowBox[{
     RowBox[{
      SubsuperscriptBox["\[Integral]", 
       RowBox[{"-", "\[Infinity]"}], "0"], 
      RowBox[{
       FractionBox["1", 
        SqrtBox[
         RowBox[{"1", "+", 
          SuperscriptBox["x", "2"]}]]], 
       RowBox[{"\[DifferentialD]", "x"}]}]}], "+", 
     RowBox[{
      SubsuperscriptBox["\[Integral]", "0", "\[Infinity]"], 
      RowBox[{
       FractionBox["1", 
        SqrtBox[
         RowBox[{"1", "+", 
          SuperscriptBox["x", "2"]}]]], 
       RowBox[{
        RowBox[{"\[DifferentialD]", "x"}], "."}]}]}]}]}], TraditionalForm]],
  ExpressionUUID->"d966e938-3b74-4e64-a52e-00111b318675"],
 "\nNow, evaluate the first improper integral on the right hand side."
}], "Text",
