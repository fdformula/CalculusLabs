Cell["\<\
An improper integral is said to be convergent if its defining limit exists; \
otherwise, it is said to be divergent.\
\>", "Text",
 CellChangeTimes->{
  3.6438978071648803`*^9},ExpressionUUID->"72de0409-412f-4641-b191-\
ecf276fb0edb"],

Cell[TextData[{
 "For ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     SubsuperscriptBox["\[Integral]", "a", "b"], 
     RowBox[{
      RowBox[{"f", "(", "x", ")"}], 
      RowBox[{"\[DifferentialD]", "x"}]}]}], "=", 
    RowBox[{
     RowBox[{
      SubsuperscriptBox["\[Integral]", 
       SubscriptBox["c", "1"], 
       SubscriptBox["c", "2"]], 
      RowBox[{
       RowBox[{"f", "(", "x", ")"}], 
       RowBox[{"\[DifferentialD]", "x"}]}]}], "+", 
     RowBox[{
      SubsuperscriptBox["\[Integral]", 
       SubscriptBox["c", "2"], 
       SubscriptBox["c", "3"]], 
      RowBox[{
       RowBox[{"f", "(", "x", ")"}], 
       RowBox[{"\[DifferentialD]", "x"}]}]}], "+", "\[CenterEllipsis]", "+", 
     RowBox[{
      SubsuperscriptBox["\[Integral]", 
       SubscriptBox["c", 
        RowBox[{"n", "-", "1"}]], 
       SubscriptBox["c", "n"]], 
      RowBox[{
       RowBox[{"f", "(", "x", ")"}], 
       RowBox[{"\[DifferentialD]", "x"}]}]}]}]}], TraditionalForm]],
  ExpressionUUID->"4b7ef092-31c9-4952-953e-59108c3ed7f3"],
 ", if any one of the improper integrals on the right hand side is divergent, \
",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubsuperscriptBox["\[Integral]", "a", "b"], 
    RowBox[{
     RowBox[{"f", "(", "x", ")"}], 
     RowBox[{"\[DifferentialD]", "x"}]}]}], TraditionalForm]],ExpressionUUID->
  "d1eade43-70ee-41b0-b4b2-d5c1b65a9172"],
 " is divergent."
}], "Text",
 CellChangeTimes->{{3.6438963883312407`*^9, 3.643896491979128*^9}, {
  3.643897743313247*^9, 
  3.6438977595401726`*^9}},ExpressionUUID->"9d98e587-df51-421a-8de2-\
abf4fab30a83"],

