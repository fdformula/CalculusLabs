Cell[TextData[{
 StyleBox["Example 8\n",
  FontWeight->"Bold"],
 "Evaluate ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubsuperscriptBox["\[Integral]", 
     RowBox[{"-", "1"}], "1"], 
    RowBox[{
     FractionBox["1", 
      SuperscriptBox["x", "2"]], 
     RowBox[{"\[DifferentialD]", "x"}]}]}], TraditionalForm]],ExpressionUUID->
  "0a503168-1418-4f78-8deb-8ca0f450ad63"],
 " and determine its convergence.\nRewrite ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     SubsuperscriptBox["\[Integral]", 
      RowBox[{"-", "1"}], "1"], 
     RowBox[{
      FractionBox["1", 
       SuperscriptBox["x", "2"]], 
      RowBox[{"\[DifferentialD]", "x"}]}]}], "=", 
    RowBox[{
     RowBox[{
      SubsuperscriptBox["\[Integral]", 
       RowBox[{"-", "1"}], "0"], 
      RowBox[{
       FractionBox["1", 
        SuperscriptBox["x", "2"]], 
       RowBox[{"\[DifferentialD]", "x"}]}]}], "+", 
     RowBox[{
      SubsuperscriptBox["\[Integral]", "0", "1"], 
      RowBox[{
       FractionBox["1", 
