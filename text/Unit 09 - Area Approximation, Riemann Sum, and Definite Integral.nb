Cell[TextData[{
 "Now, we can find the area between the two curves on ",
 Cell[BoxData[
  FormBox[
   RowBox[{"[", 
    RowBox[{"1", ",", 
     SubscriptBox["x", "0"]}], "]"}], TraditionalForm]],ExpressionUUID->
  "e14bb89b-49ba-471f-aba0-8a9ad3b6c5ee"],
 " by ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubsuperscriptBox["\[Integral]", "1", 
     SubscriptBox["x", "0"]], 
    RowBox[{
     RowBox[{"(", 
      RowBox[{
       RowBox[{"x", " ", "sin", " ", "x"}], "-", 
       RowBox[{
        FractionBox["1", "4"], 
        SuperscriptBox["x", "2"]}]}], ")"}], 
     RowBox[{"\[DifferentialD]", "x"}]}]}], TraditionalForm]],ExpressionUUID->
  "f0aa2947-a5c3-47c2-8b7b-dc6b087d87d8"],
 " and the area on ",
 Cell[BoxData[
  FormBox[
   RowBox[{"[", 
    RowBox[{
     SubscriptBox["x", "0"], ",", "3"}], "]"}], TraditionalForm]],
  ExpressionUUID->"7006617b-2165-4343-ac4a-5d6169d02e96"],
 " by ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubsuperscriptBox["\[Integral]", 
     SubscriptBox["x", "0"], "3"], 
    RowBox[{
     RowBox[{"(", 
      RowBox[{
       RowBox[{
        FractionBox["1", "4"], 
        SuperscriptBox["x", "2"]}], "-", 
       RowBox[{"x", " ", "sin", " ", "x"}]}], ")"}], 
     RowBox[{"\[DifferentialD]", "x"}]}]}], TraditionalForm]],ExpressionUUID->
  "1af222c8-784b-45e3-9b5e-a8e85d8ea84e"],
