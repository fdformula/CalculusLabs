Cell[TextData[{
 StyleBox["Example 5\n",
  FontWeight->"Bold"],
 "Write a ",
 StyleBox["Mathematica",
  FontSlant->"Italic"],
 " function that finds the local extrema of any polynomial function.\n\nSee \
notes in Example 4."
}], "Text",
 CellDingbat->"\[FilledSquare]",
 CellChangeTimes->{{3.6427650774724007`*^9, 
  3.642765147816141*^9}},ExpressionUUID->"5979d377-5302-4c85-94a7-\
32c24203bdec"],

Cell[BoxData[{
 RowBox[{
  RowBox[{
   RowBox[{"FindLocalExtremaOfDifferentiableFunction", "[", 
    RowBox[{"func_", ",", 
     RowBox[{"{", 
      RowBox[{"x_", ",", "a_", ",", "b_"}], "}"}], ",", "plotQ_"}], "]"}], ":=", 
   RowBox[{"Module", "[", "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{"{", 
      RowBox[{
      "f", ",", "fPrime", ",", "fDPrime", ",", "cps", ",", "xs", ",", "i", 
       ",", "xMaxs", ",", "xMins"}], "}"}], ",", "\[IndentingNewLine]", 
     "\[IndentingNewLine]", 
     RowBox[{
      RowBox[{
       RowBox[{"f", "[", "X_", "]"}], "=", 
       RowBox[{"func", "/.", 
        RowBox[{"x", "\[Rule]", "X"}]}]}], ";", "\[IndentingNewLine]", 
      RowBox[{
       RowBox[{"fPrime", "[", "X_", "]"}], "=", 
       RowBox[{"D", "[", 
        RowBox[{
         RowBox[{"f", "[", "X", "]"}], ",", "X"}], "]"}]}], ";", 
      "\[IndentingNewLine]", 
      RowBox[{
       RowBox[{"fDPrime", "[", "X_", "]"}], "=", 
       RowBox[{"D", "[", 
        RowBox[{
         RowBox[{"fPrime", "[", "X", "]"}], ",", "X"}], "]"}]}], ";", 
      "\[IndentingNewLine]", "\[IndentingNewLine]", 
      RowBox[{"cps", "=", 
       RowBox[{"x", "/.", 
        RowBox[{"NSolve", "[", 
         RowBox[{
          RowBox[{
           RowBox[{
            RowBox[{"fPrime", "[", "x", "]"}], "\[Equal]", "0"}], "&&", 
           RowBox[{"a", "<", "x", "<", "b"}]}], ",", "x", ",", "Reals"}], 
         "]"}]}]}], ";", 
      RowBox[{"(*", " ", 
       RowBox[{"critical", " ", "points"}], " ", "*)"}], 
      "\[IndentingNewLine]", "\[IndentingNewLine]", 
      RowBox[{"(*", " ", 
       RowBox[{
       "apply", " ", "the", " ", "second", " ", "derivative", " ", "test"}], 
       " ", "*)"}], "\[IndentingNewLine]", 
      RowBox[{"xMaxs", "=", 
       RowBox[{"Select", "[", 
        RowBox[{"cps", ",", 
         RowBox[{
          RowBox[{
           RowBox[{"fDPrime", "[", "#", "]"}], "<", "0"}], "&"}]}], "]"}]}], 
      ";", " ", 
      RowBox[{"(*", " ", 
       RowBox[{"critical", " ", "points", " ", "where", " ", "f", 
        RowBox[{"(", "x", ")"}], " ", "has", " ", "local", " ", "max", " ", 
        "value"}], " ", "*)"}], "\[IndentingNewLine]", 
      RowBox[{"xMins", "=", 
       RowBox[{"Select", "[", 
        RowBox[{"cps", ",", 
         RowBox[{
          RowBox[{
           RowBox[{"fDPrime", "[", "#", "]"}], ">", "0"}], "&"}]}], "]"}]}], 
      ";", " ", 
      RowBox[{"(*", " ", 
       RowBox[{"critical", " ", "points", " ", "where", " ", "f", 
        RowBox[{"(", "x", ")"}], " ", "has", " ", "local", " ", "min", " ", 
        "value"}], " ", "*)"}], "\[IndentingNewLine]", 
      RowBox[{"Print", "[", 
       RowBox[{"\"\<Local maximum values \>\"", ",", " ", 
        RowBox[{"Table", "[", 
         RowBox[{
          RowBox[{"{", 
           RowBox[{
            RowBox[{"f", "[", 
             RowBox[{"xMaxs", "[", 
              RowBox[{"[", "i", "]"}], "]"}], "]"}], ",", 
            RowBox[{"{", 
             RowBox[{"x", "\[Rule]", 
              RowBox[{"xMaxs", "[", 
               RowBox[{"[", "i", "]"}], "]"}]}], "}"}]}], "}"}], ",", 
          RowBox[{"{", 
           RowBox[{"i", ",", "1", ",", 
            RowBox[{"Length", "[", "xMaxs", "]"}]}], "}"}]}], "]"}]}], "]"}], 
      ";", "\[IndentingNewLine]", 
      RowBox[{"Print", "[", 
       RowBox[{"\"\<Local maximum values \>\"", ",", " ", 
        RowBox[{"Table", "[", 
         RowBox[{
          RowBox[{"{", 
           RowBox[{
            RowBox[{"f", "[", 
             RowBox[{"xMins", "[", 
              RowBox[{"[", "i", "]"}], "]"}], "]"}], ",", 
            RowBox[{"{", 
             RowBox[{"x", "\[Rule]", 
              RowBox[{"xMins", "[", 
               RowBox[{"[", "i", "]"}], "]"}]}], "}"}]}], "}"}], ",", 
          RowBox[{"{", 
           RowBox[{"i", ",", "1", ",", 
            RowBox[{"Length", "[", "xMins", "]"}]}], "}"}]}], "]"}]}], "]"}], 
      ";", "\[IndentingNewLine]", "\[IndentingNewLine]", 
      RowBox[{"If", "[", 
       RowBox[{
        RowBox[{"plotQ", "\[Equal]", "True"}], ",", "\[IndentingNewLine]", 
        RowBox[{"Module", "[", " ", 
         RowBox[{
          RowBox[{"{", 
           RowBox[{"xmax", ",", "xmin"}], "}"}], ",", "\[IndentingNewLine]", 
          RowBox[{
           RowBox[{"xmin", "=", 
            RowBox[{"If", "[", 
             RowBox[{
              RowBox[{"a", "\[Equal]", 
               RowBox[{"-", "\[Infinity]"}]}], ",", 
              RowBox[{
               RowBox[{"Min", "[", "cps", "]"}], "-", "1"}], ",", "a"}], 
             "]"}]}], ";", "\[IndentingNewLine]", 
           RowBox[{"xmax", "=", 
            RowBox[{"If", "[", 
             RowBox[{
              RowBox[{"b", "\[Equal]", "\[Infinity]"}], ",", 
              RowBox[{
               RowBox[{"Max", "[", "cps", "]"}], "+", "1"}], ",", "b"}], 
             "]"}]}], ";", "\[IndentingNewLine]", 
           RowBox[{"Show", "[", 
            RowBox[{"{", "\[IndentingNewLine]", 
             RowBox[{
              RowBox[{"Plot", "[", 
               RowBox[{
                RowBox[{"f", "[", "x", "]"}], ",", 
                RowBox[{"{", 
                 RowBox[{"x", ",", "xmin", ",", "xmax"}], "}"}], ",", 
                RowBox[{"ImageSize", "\[Rule]", "250"}]}], "]"}], ",", 
              "\[IndentingNewLine]", 
              RowBox[{"Table", "[", 
               RowBox[{
                RowBox[{"Graphics", "[", 
                 RowBox[{"{", 
                  RowBox[{"Red", ",", 
                   RowBox[{"PointSize", "[", "Medium", "]"}], ",", 
                   RowBox[{"Point", "[", 
                    RowBox[{"{", 
                    RowBox[{
                    RowBox[{"xMaxs", "[", 
                    RowBox[{"[", "i", "]"}], "]"}], ",", 
                    RowBox[{"f", "[", 
                    RowBox[{"xMaxs", "[", 
                    RowBox[{"[", "i", "]"}], "]"}], "]"}]}], "}"}], "]"}]}], 
                  "}"}], "]"}], ",", 
                RowBox[{"{", 
                 RowBox[{"i", ",", "1", ",", 
                  RowBox[{"Length", "[", "xMaxs", "]"}]}], "}"}]}], "]"}], 
              ",", "\[IndentingNewLine]", 
              RowBox[{"Table", "[", 
               RowBox[{
                RowBox[{"Graphics", "[", 
                 RowBox[{"{", 
                  RowBox[{"Blue", ",", 
                   RowBox[{"PointSize", "[", "Medium", "]"}], ",", 
                   RowBox[{"Point", "[", 
                    RowBox[{"{", 
                    RowBox[{
                    RowBox[{"xMins", "[", 
                    RowBox[{"[", "i", "]"}], "]"}], ",", 
                    RowBox[{"f", "[", 
                    RowBox[{"xMins", "[", 
                    RowBox[{"[", "i", "]"}], "]"}], "]"}]}], "}"}], "]"}]}], 
                  "}"}], "]"}], ",", 
                RowBox[{"{", 
                 RowBox[{"i", ",", "1", ",", 
                  RowBox[{"Length", "[", "xMins", "]"}]}], "}"}]}], "]"}]}], 
             "\[IndentingNewLine]", "}"}], "]"}]}]}], "\[IndentingNewLine]", 
         "]"}]}], "]"}]}]}], "\[IndentingNewLine]", "]"}]}], 
  "\[IndentingNewLine]"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"FindPolynomialLocalExtrema", "[", 
    RowBox[{"func_", ",", "plotQ_"}], "]"}], " ", ":=", " ", 
   RowBox[{"FindLocalExtremaOfDifferentiableFunction", "[", 
    RowBox[{"func", ",", 
     RowBox[{"{", 
      RowBox[{"x", ",", 
       RowBox[{"-", "\[Infinity]"}], ",", "\[Infinity]"}], "}"}], ",", 
     "plotQ"}], "]"}]}], ";"}]}], "Input",
