Cell[BoxData[
 RowBox[{
  RowBox[{"FindPolynomialAbsoluteExtrema", "[", 
   RowBox[{"func_", ",", 
    RowBox[{"{", 
     RowBox[{"x_", ",", "a_", ",", "b_"}], "}"}], ",", "plotQ_"}], "]"}], ":=", 
  RowBox[{"Module", "[", "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{
     "f", ",", "cps", ",", "xs", ",", "fValues", ",", "iMax", ",", "iMaxs", 
      ",", " ", "iMin", ",", "iMins", ",", "i"}], "}"}], ",", 
    "\[IndentingNewLine]", "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{
      RowBox[{"f", "[", "X_", "]"}], ":=", 
      RowBox[{"func", "/.", 
       RowBox[{"x", "\[Rule]", "X"}]}]}], ";", "\[IndentingNewLine]", 
     RowBox[{"cps", "=", 
      RowBox[{"x", "/.", 
       RowBox[{"NSolve", "[", 
        RowBox[{
         RowBox[{
          RowBox[{
           RowBox[{
            RowBox[{"f", "'"}], "[", "x", "]"}], "\[Equal]", "0"}], "&&", 
          RowBox[{"a", "\[LessEqual]", "x", "\[LessEqual]", "b"}]}], ",", 
         "x"}], "]"}]}]}], ";", 
     RowBox[{"(*", " ", 
      RowBox[{"all", " ", "critical", " ", "points", " ", 
       RowBox[{"in", " ", "[", 
        RowBox[{"a", ",", " ", "b"}], "]"}]}], " ", "*)"}], 
     "\[IndentingNewLine]", "\[IndentingNewLine]", 
     RowBox[{"(*", " ", 
      RowBox[{"evaluate", " ", "f", 
       RowBox[{"(", "x", ")"}], " ", "at", " ", "critical", " ", "points", 
       " ", "as", " ", "well", " ", "as", " ", "the", " ", "endpoints", " ", 
       RowBox[{"of", " ", "[", 
        RowBox[{"a", ",", " ", "b"}], "]"}]}], " ", "*)"}], 
     "\[IndentingNewLine]", 
     RowBox[{"xs", " ", "=", 
      RowBox[{"Sort", "[", " ", 
       RowBox[{"Flatten", "[", 
        RowBox[{"{", 
         RowBox[{"cps", ",", "a", ",", "b"}], "}"}], "]"}], "]"}]}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"fValues", "=", 
      RowBox[{"f", "[", "xs", "]"}]}], ";", "\[IndentingNewLine]", 
     "\[IndentingNewLine]", 
     RowBox[{"(*", " ", 
      RowBox[{"find", " ", "absolute", " ", "maximum", " ", "values"}], " ", 
      "*)"}], "\[IndentingNewLine]", 
     RowBox[{"iMax", "=", "1"}], ";", "\[IndentingNewLine]", 
     RowBox[{"iMaxs", " ", "=", " ", 
      RowBox[{"{", "1", "}"}]}], ";", 
     RowBox[{"(*", " ", 
      RowBox[{
      "there", " ", "may", " ", "be", " ", "multiple", " ", "points", " ", 
       "where", " ", "f", 
       RowBox[{"(", "x", ")"}], " ", "has", " ", "the", " ", "absolute", " ", 
       "maximum", " ", "value"}], " ", "*)"}], "\[IndentingNewLine]", 
     RowBox[{"Do", "[", 
      RowBox[{
       RowBox[{"If", "[", 
        RowBox[{
         RowBox[{
          RowBox[{"fValues", "[", 
           RowBox[{"[", "i", "]"}], "]"}], ">", 
          RowBox[{"fValues", "[", 
           RowBox[{"[", "iMax", "]"}], "]"}]}], ",", 
         RowBox[{
          RowBox[{"iMax", "=", "i"}], ";", 
          RowBox[{"iMaxs", "=", 
           RowBox[{"{", "i", "}"}]}]}], ",", "\[IndentingNewLine]", 
         RowBox[{"If", "[", 
          RowBox[{
           RowBox[{
            RowBox[{"Abs", "[", 
             RowBox[{
              RowBox[{"fValues", "[", 
               RowBox[{"[", "i", "]"}], "]"}], "-", 
              RowBox[{"fValues", "[", 
               RowBox[{"[", "iMax", "]"}], "]"}]}], "]"}], "<", 
            SuperscriptBox["10", 
             RowBox[{"-", "16"}]]}], ",", 
           RowBox[{"(*", " ", 
            RowBox[{
             RowBox[{"with", " ", "floating", " ", "point", " ", "numbers"}], 
             ",", " ", 
             RowBox[{
              RowBox[{"we", " ", "cannot", " ", "use", " ", 
               RowBox[{"fValues", "[", "i", "]"}]}], "==", 
              RowBox[{"fValues", "[", 
               RowBox[{"[", "iMax", "]"}], "]"}]}]}], " ", "*)"}], 
           "\[IndentingNewLine]", 
           RowBox[{"iMaxs", "=", 
            RowBox[{"Append", "[", 
             RowBox[{"iMaxs", ",", "i"}], "]"}]}]}], "]"}]}], "]"}], ",", 
       "\[IndentingNewLine]", 
       RowBox[{"{", 
        RowBox[{"i", ",", "2", ",", 
         RowBox[{"Length", "[", "xs", "]"}]}], "}"}]}], "]"}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"Print", "[", 
      RowBox[{"\"\<Absolute maximum value \>\"", ",", 
       RowBox[{"{", 
        RowBox[{
         RowBox[{"fValues", "[", 
          RowBox[{"[", "iMax", "]"}], "]"}], ",", 
         RowBox[{"Table", "[", 
          RowBox[{
           RowBox[{"x", "\[Rule]", 
            RowBox[{"xs", "[", 
             RowBox[{"[", 
              RowBox[{"iMaxs", "[", 
               RowBox[{"[", "i", "]"}], "]"}], "]"}], "]"}]}], ",", 
           RowBox[{"{", 
            RowBox[{"i", ",", "1", ",", 
             RowBox[{"Length", "[", "iMaxs", "]"}]}], "}"}]}], "]"}]}], 
        "}"}]}], "]"}], ";", "\[IndentingNewLine]", "\[IndentingNewLine]", 
     RowBox[{"(*", " ", 
      RowBox[{"find", " ", "absolute", " ", "minimum", " ", "values"}], " ", 
      "*)"}], "\[IndentingNewLine]", 
     RowBox[{"iMin", "=", "1"}], ";", "\[IndentingNewLine]", 
     RowBox[{"iMins", "=", 
      RowBox[{"{", "1", "}"}]}], ";", " ", 
     RowBox[{"(*", " ", 
      RowBox[{
      "there", " ", "may", " ", "be", " ", "multiple", " ", "points", " ", 
       "where", " ", "f", 
       RowBox[{"(", "x", ")"}], " ", "has", " ", "the", " ", "absolute", " ", 
       "minimum", " ", "value"}], " ", "*)"}], "\[IndentingNewLine]", 
     RowBox[{"Do", "[", 
      RowBox[{
       RowBox[{"If", "[", 
        RowBox[{
         RowBox[{
          RowBox[{"fValues", "[", 
           RowBox[{"[", "i", "]"}], "]"}], "<", 
          RowBox[{"fValues", "[", 
           RowBox[{"[", "iMin", "]"}], "]"}]}], ",", 
         RowBox[{
          RowBox[{"iMin", "=", "i"}], ";", 
          RowBox[{"iMins", "=", 
           RowBox[{"{", "i", "}"}]}]}], ",", "\[IndentingNewLine]", 
         RowBox[{"If", "[", 
          RowBox[{
           RowBox[{
            RowBox[{"Abs", "[", 
             RowBox[{
              RowBox[{"fValues", "[", 
               RowBox[{"[", "i", "]"}], "]"}], "-", 
              RowBox[{"fValues", "[", 
               RowBox[{"[", "iMin", "]"}], "]"}]}], "]"}], "<", 
            SuperscriptBox["10", 
             RowBox[{"-", "16"}]]}], ",", 
           RowBox[{"(*", " ", 
            RowBox[{
             RowBox[{"with", " ", "floating", " ", "point", " ", "numbers"}], 
             ",", " ", 
             RowBox[{
              RowBox[{"we", " ", "cannot", " ", "use", " ", 
               RowBox[{"fValues", "[", "i", "]"}]}], "==", 
              RowBox[{"f", "[", "iMax", "]"}]}]}], " ", "*)"}], 
           "\[IndentingNewLine]", 
           RowBox[{"iMins", "=", 
            RowBox[{"Append", "[", 
             RowBox[{"iMins", ",", "i"}], "]"}]}]}], "]"}]}], "]"}], ",", 
       "\[IndentingNewLine]", 
       RowBox[{"{", 
        RowBox[{"i", ",", "2", ",", 
         RowBox[{"Length", "[", "xs", "]"}]}], "}"}]}], "]"}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"Print", "[", 
      RowBox[{"\"\<Absolute minimum value \>\"", ",", 
       RowBox[{"{", 
        RowBox[{
         RowBox[{"fValues", "[", 
          RowBox[{"[", "iMin", "]"}], "]"}], ",", 
         RowBox[{"Table", "[", 
          RowBox[{
           RowBox[{"x", "\[Rule]", 
            RowBox[{"xs", "[", 
             RowBox[{"[", 
              RowBox[{"iMins", "[", 
               RowBox[{"[", "i", "]"}], "]"}], "]"}], "]"}]}], ",", 
           RowBox[{"{", 
            RowBox[{"i", ",", "1", ",", 
             RowBox[{"Length", "[", "iMins", "]"}]}], "}"}]}], "]"}]}], 
        "}"}]}], "]"}], ";", "\[IndentingNewLine]", "\[IndentingNewLine]", 
     RowBox[{"If", "[", 
      RowBox[{
       RowBox[{"plotQ", "\[Equal]", "True"}], ",", "\[IndentingNewLine]", 
       RowBox[{"Show", "[", 
        RowBox[{"{", 
